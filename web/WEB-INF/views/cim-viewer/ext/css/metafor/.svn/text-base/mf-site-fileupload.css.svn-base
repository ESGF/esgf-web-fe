/* ************************** */
/*  File uploader specific    */
/* ************************** */
.mf-fileBrowser {
    padding: 5px;
    height:40px;
}

.mf-fileSelector {
    margin-top: 6px;
}

.mf-fileNameLabel
{
    margin-right: 6px;
    margin-top: 8px;
}

.mf-fileName
{
    width: 650px;
    margin-top: 4px;
}

.mf-fileButtonsPanel {
    position: relative;
}

.mf-fileBrowseButton {
    margin-right: 10px;
    margin-top: 6px;
}

.mf-fileSubmitButton {
    width: 120px;
    margin-top: 6px;
    margin-right: 6px;
}

.mf-fileUploader
{
    font-size: 24px;
    position: absolute;
    width: 70px;
    top: 0;
    opacity: 0;
    left:0;
    filter: alpha(opacity=0);
    -ms-filter: "alpha(opacity=0)";
    -khtml-opacity: 0;
    -moz-opacity: 0;
}

.mf-fileReport {
    margin-top: 5px;
}